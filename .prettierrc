{
  "arrowParens": "always",
  "importOrder": [
    "^(react|next).*",
    "<BUILTIN_MODULES>",
    "<THIRD_PARTY_MODULES>",
    "<TYPES>"
  ],
  "plugins": [
    "prettier-plugin-imports",
    "prettier-plugin-jsdoc",
    "prettier-plugin-pkg",
    "prettier-plugin-tailwindcss"
  ],
  "quoteProps": "consistent",
  "semi": true,
  "tailwindFunctions": ["clsx", "cn"],
  "trailingComma": "all"
}
